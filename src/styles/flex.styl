.flex-row {
    display flex
    flex-flow: row nowrap
    align-items: center
    }
.flex-wrap
    flex-flow: row nowrap
.flex-row-wrap {
    display flex
    flex-flow: row wrap
    align-items: center
    }
.justify-between {
    justify-content: space-between
    }
.justify-start {
    justify-content: flex-start
    }
.justify-around {
    justify-content: space-around
    }
.align-center {
    align-items: center
    }
.self-start
    align-self: flex-start
